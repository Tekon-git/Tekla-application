<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Kolor-->
    <SolidColorBrush x:Key="NIPDarkGreyBrush" Color="#363545"></SolidColorBrush>
    <SolidColorBrush x:Key="NIPWhiteBrush" Color="#F3F3F7"></SolidColorBrush>
    <SolidColorBrush x:Key="NIPGoldBrush" Color="#FFBE00"></SolidColorBrush>

    <Style TargetType="Button">
        <Setter Property="Background" Value="{StaticResource NIPDarkGreyBrush}"></Setter>
        <Setter Property="Foreground" Value="{StaticResource NIPGoldBrush}"></Setter>
    </Style>
    <Geometry x:Key="MinimizeButtonIconGeometry">M0,8 H8 M0,7 H8</Geometry>
    <Geometry x:Key="CloseButtonIconGeometry">M0,0 L8,8 M0,8 L8,0</Geometry>
    
</ResourceDictionary>